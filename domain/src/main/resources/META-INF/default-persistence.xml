<?xml version="1.0" encoding="UTF-8"?>
<persistence:persistence version="1.0" xmlns:persistence="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/persistence ../../persistence_1_0.xsd ">
  <!-- Only used by eclipselink-staticweave-maven-plugin -->
  <persistence:persistence-unit name="default">
    <persistence:provider>org.eclipse.persistence.jpa.PersistenceProvider</persistence:provider>

    <persistence:class>net.sf.gazpachosurvey.domain.core.AnonymousInvitation</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.core.Answer</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.core.Invitation</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.core.Label</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.core.LabelSet</persistence:class>

    <persistence:class>net.sf.gazpachosurvey.domain.core.MailMessage</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.core.MailMessageTemplate</persistence:class>

    <persistence:class>net.sf.gazpachosurvey.domain.core.Page</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.core.Participant</persistence:class>

    <persistence:class>net.sf.gazpachosurvey.domain.core.PersonalInvitation</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.core.Question</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.core.Respondent</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.core.Survey</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.core.SurveyInstance</persistence:class>
    <!-- User -->
    <persistence:class>net.sf.gazpachosurvey.domain.user.User</persistence:class>
    <!-- i18 -->
    <persistence:class>net.sf.gazpachosurvey.domain.i18.AnswerTranslation</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.i18.LabelTranslation</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.i18.MailMessageTemplateTranslation</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.i18.PageTranslation</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.i18.QuestionTranslation</persistence:class>
    <persistence:class>net.sf.gazpachosurvey.domain.i18.SurveyTranslation</persistence:class>
    <!-- Support -->
    <persistence:class>net.sf.gazpachosurvey.domain.support.AbstractInvitation</persistence:class>

    <persistence:properties>
      <persistence:property name="eclipselink.jdbc.driver" value="org.postgresql.Driver" />
      <persistence:property name="eclipselink.jdbc.url" value="jdbc:postgresql://localhost:5432/gazpachosurvey" />
      <persistence:property name="eclipselink.jdbc.user" value="postgres" />
      <persistence:property name="eclipselink.jdbc.password" value="admin" />
      <persistence:property name="eclipselink.weaving" value="static"></persistence:property>
    </persistence:properties>
  </persistence:persistence-unit>
</persistence:persistence>