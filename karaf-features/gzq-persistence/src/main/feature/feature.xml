<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<features name="gzq-persistence" xmlns="http://karaf.apache.org/xmlns/features/v1.2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://karaf.apache.org/xmlns/features/v1.2.0 http://karaf.apache.org/xmlns/features/v1.2.0">

  <feature name="gzq-persistence" description="GazpachoQuest Domain">
    <feature version="${project.version}">gzq-ext-eclipselink</feature>
    <feature>pax-jdbc-postgresql</feature>
    <feature>pax-jdbc-config</feature>
    <feature>pax-jdbc-pool-dbcp2</feature>

    <feature>shiro-core</feature>
    <feature version="[4.0.7.RELEASE_1,4.1)">spring</feature>
    <bundle start-level="150">mvn:net.sf.gazpachoquest/gazpachoquest-types/${project.version}</bundle>
    <bundle start-level="155">mvn:net.sf.gazpachoquest.persistence/gazpachoquest-jpa/${project.version}</bundle>
    <bundle start-level="155">mvn:net.sf.gazpachoquest.persistence/gazpachoquest-domain/${project.version}</bundle>

  </feature>

</features>
